USE sql_store;

SELECT * FROM sql_store.customers WHERE state IN ("VA", "TN" , "GA");
SELECT * FROM sql_store.customers WHERE state NOT IN ("VA", "TN" , "GA");

-- Exercise 
-- Retrun product with
-- quantity in stock equal to 49 , 38 , 72

SELECT *
	FROM products
    WHERE quantity_in_stock IN (49 , 38 , 72)